                <p> {{ section.settings.dog_health_info }}</p>
                <ul>
                  <li class="smail-text-size-cstm">
                    <span> {{ section.settings.table_top-h }} </span>
                    <span> {{ section.settings.table_top-h1 }} </span>
                  </li>
                  <li>
                    <span> {{ section.settings.table_info }} </span>
                    <span> {{ section.settings.table_info1 }} </span>
                  </li>
                   <li>
                    <span> {{ section.settings.table_info2 }} </span>
